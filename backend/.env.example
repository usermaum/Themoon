# Render.com 배포 설정

# 로컬 개발: SQLite 사용 (간단)
DATABASE_URL=sqlite:///./themoon.db

# 로컬 개발: Render PostgreSQL 사용 (External URL 필요)
# DATABASE_URL=postgresql://themoon:PASSWORD@dpg-xxxxx-oregon-postgres.render.com/themoon

# CORS 설정
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:3001"]

# 디버그 모드
DEBUG=true

# 시크릿 키 (로컬 개발용)
SECRET_KEY=your-local-secret-key-change-this
